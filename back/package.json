{
  "name": "anssi-demain-specialiste-cyber-back",
  "version": "2.0.0",
  "description": "Backend « DemainSpécialisteCyber »",
  "main": "src/serveur.ts",
  "scripts": {
    "dev": "npm-run-all --parallel --race dev:bdd dev:server",
    "dev:bdd": "docker compose up db",
    "dev:server": "sleep 3 && pnpm migre-bdd && node --import tsx --env-file .env --watch src/serveur.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prestart": "tsc",
    "build": "tsc",
    "start": "node ./dist/src/serveur.js",
    "pretest": "npm-run-all --parallel typecheck lint",
    "test": "vitest run",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit --project ./tsconfig.typecheck.json",
    "cree-migration": "knex migrate:make -x ts",
    "migre-bdd": "node --env-file=.env --import tsx ./node_modules/knex/bin/cli.js migrate:latest",
    "migre-bdd:clever": "node ./node_modules/knex/bin/cli.js migrate:latest  --knexfile ./dist/knexfile.js"
  },
  "keywords": [],
  "author": "Lab de l’ANSSI",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "~3.901.0",
    "@aws-sdk/credential-providers": "~3.901.0",
    "@lab-anssi/lib": "~2.1.2",
    "@sentry/node": "~10.27.0",
    "bcrypt": "~6.0.0",
    "cookie-parser": "~1.4.7",
    "cookie-session": "~2.1.1",
    "date-fns": "~4.1.0",
    "express": "~4.22.1",
    "helmet": "~8.1.0",
    "jsonwebtoken": "~9.0.2",
    "knex": "~3.1.0",
    "multer": "~2.0.2",
    "openid-client": "~5.7.1",
    "pg": "~8.16.3",
    "pug": "~3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "~9.36.0",
    "@types/bcrypt": "~6.0.0",
    "@types/cookie-parser": "~1.4.9",
    "@types/cookie-session": "~2.0.49",
    "@types/express": "~5.0.3",
    "@types/express-serve-static-core": "~5.1.0",
    "@types/jsonwebtoken": "~9.0.10",
    "@types/multer": "~2.0.0",
    "@types/node": "~24.5.2",
    "@types/supertest": "~6.0.3",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-prettier": "~5.5.4",
    "jiti": "~2.6.0",
    "node-mocks-http": "~1.17.2",
    "supertest": "~7.1.4",
    "ts-node": "~10.9.2",
    "tsx": "~4.20.5",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.44.1",
    "vitest": "~3.2.4"
  }
}
