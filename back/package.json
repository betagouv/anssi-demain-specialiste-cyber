{
  "name": "anssi-demain-specialiste-cyber",
  "version": "2.0.0",
  "description": "Backend « DemainSpécialisteCyber »",
  "main": "src/serveur.ts",
  "scripts": {
    "dev": "npm-run-all --parallel --race dev:bdd dev:server",
    "dev:bdd": "docker compose up db",
    "dev:server": "sleep 3 && npm run migre-bdd && node --import tsx --env-file .env --watch src/serveur.ts",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prestart": "tsc",
    "build": "tsc",
    "start": "node ./dist/src/serveur.js",
    "pretest": "npm-run-all --parallel typecheck lint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --project ./tsconfig.typecheck.json",
    "cree-migration": "knex migrate:make -x ts",
    "migre-bdd": "node --env-file=.env --import tsx ../node_modules/knex/bin/cli.js migrate:latest",
    "migre-bdd:clever": "node ../node_modules/knex/bin/cli.js migrate:latest  --knexfile ./dist/knexfile.js"
  },
  "keywords": [],
  "author": "Lab de l’ANSSI",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/credential-providers": "^3.901.0",
    "@lab-anssi/lib": "^2.1.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "openid-client": "^5.7.1",
    "pg": "^8.16.3",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.5.1",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}
